# Calculate path=~/.dconfigure.d chmod=700 ini(update.dconf)!=off&&ini(update.mate)!=off&&pkg(mate-base/mate-session-manager)>1.6
#!/bin/bash

## Starting by ~/.config/autostart/dconf.desktop

#?env(client.os_remote_auth)==||ur_domain_set==off#
dconf write /org/mate/caja/desktop/home-icon-visible true &>/dev/null
dconf write /org/mate/caja/desktop/computer-icon-visible true &>/dev/null
dconf write /org/mate/caja/desktop/volumes-visible true &>/dev/null
dconf write /org/mate/caja/desktop/network-icon-visible true &>/dev/null
dconf write /org/mate/caja/desktop/trash-icon-visible true &>/dev/null
#env#
#?env(client.os_remote_auth)!=&&ur_domain_set==on#
dconf write /org/mate/caja/desktop/home-icon-visible false &>/dev/null
dconf write /org/mate/caja/desktop/computer-icon-visible false
dconf write /org/mate/caja/desktop/volumes-visible false
dconf write /org/mate/caja/desktop/network-icon-visible false
dconf write /org/mate/caja/desktop/trash-icon-visible false

#env#
